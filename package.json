{
  "name": "react-box-layout",
  "version": "1.0.0",
  "description": "React Layout Containers",
  "main": "index.js",
  "scripts": {
    "build-demo1": "node_modules/.bin/browserify src/js/demo/demo1.js --debug -t babelify -o demo/demo1bundle.js",
    "build-demo2": "node_modules/.bin/browserify src/js/demo/demo2.js --debug -t babelify -o demo/demo2bundle.js",
    "build-lib": "node_modules/.bin/babel src/js/layout --out-dir lib",
